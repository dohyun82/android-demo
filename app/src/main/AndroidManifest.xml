            android:label="@string/title_activity_tutorial_demo"
            android:theme="@style/Theme.AndroidDemo" />
        <activity
            android:name=".biometric.BiometricActivity"
            android:exported="false"
            android:theme="@style/Theme.AndroidDemo" />
